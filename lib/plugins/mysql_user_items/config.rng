<?xml version="1.0" encoding="utf-8"?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0"
         datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
    <start>
        <element name="user_items">
            <element name="module">
                <value>mysql_user_items</value>
            </element>
            <element name="max_num_favorites">
                <attribute name="extension-by">
                    <value>mysql</value>
                </attribute>
                <data type="positiveInteger" />
            </element>
            <optional>
                <element name="mysql_host">
                    <text />
                </element>
                <element name="mysql_db">
                    <text />
                </element>
                <element name="mysql_user">
                    <text />
                </element>
                <element name="mysql_passwd">
                    <text />
                </element>
                <element name="mysql_pool_size">
                    <data type="positiveInteger" />
                </element>
                <element name="mysql_retry_delay">
                    <data type="positiveInteger" />
                </element>
                <element name="mysql_retry_attempts">
                    <data type="positiveInteger" />
                </element>
            </optional>
        </element>
    </start>
</grammar>
